        <!-- top card-->
        <div class="col col-md-12">
            <div class="card card-outline-success">
                <div class="card-header">
                    <!-- inline header form-->
                    <form class="form-inline">
                        <!-- Button trigger modal -->
                        <button id="howToUse" type="button" class="btn btn-sm btn-info ml-2 p-1" data-toggle="modal" data-target="#help_placenta">How to Use</button>
                        <!-- Histology help Modal -->
                        {{> page-modals/surg-path/placenta-how-to}}
                    </form>
                    <!--Date-->
                </div>
                <form id="plac_save">
                    <div class="card-block row text-sm-center" style="font-size: 0.8em">
                        <div class="col col-md-3">
                            <div class="card">
                                <div class="card-header h-100 text-sm-center">
                                    Gestational Age (weeks)
                                </div>
                                <div class="card-block ">
                                    <input type="number" min="19" max="44" class="form-control mx-auto" id="plac_age" placeholder="19-44">
                                </div>
                            </div>
                        </div>

                        <div class='col col-md-2'>
                            <div class="card">
                                <div class="card-header h-100 px-2 text-sm-center">
                                    Weight (grams)
                                </div>
                                <div class="card-block ">
                                    <div>
                                        <input type="number" class="form-control col-12 mx-auto" id="plac_wt">
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class='col col-md-2'>
                            <div class="card">
                                <div class="card-header h-100 px-2 text-sm-center">
                                    Gestation Type
                                </div>
                                <div class="card-block ">
                                    <div>
                                        <select id="plac_type" class="form-control">
                                            <option value="partType500" data-twin="0">Single</option>
                                            <option value="partType501" data-twin="1">Twin</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col col-md-5">
                            <div class="card">
                                <div class="card-header h-100 text-sm-center">
                                    Preferred Historic Reference
                                </div>
                                <div class="card-block ">
                                    <div>
                                        <select id="reference" class="form-control">
                                            <option data-ref="Pinar H. et al. Pediatr Pathol Lab med 1996; 16:901-7." value="1">Pinar et al. - Singleton/Twin (1996)</option>
                                            <option data-ref="Boyd T. et al. Mod Pathol 1999; 12:1P." value="2">Boyd et al. - Singleton (1999)</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                    {{#if isAuth.check}}
                    <div class="card-block row text-center" style="font-size: 0.8em">
                        <div class="col-12">
                            <div class="checkbox_container form-check form-control alert alert-success" style="margin:0;" role="alert">
