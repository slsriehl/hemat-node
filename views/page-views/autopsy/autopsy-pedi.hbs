                <!-- External exam-->
                <div class="card">
                    <div class="card-header" role="tab" id="headingThree">
                        <h5 class="mb-0">
                            <a class="collapsed btn-block" data-toggle="collapse" data-parent="#accordion"
                               href="#collapseThree" aria-expanded="true" aria-controls="collapseThree">
                                External exam
                            </a>
                        </h5>
                    </div>
                    <div id="collapseThree" class="collapse " role="tabpanel" aria-labelledby="headingOne">
                        <div class="card-block chkboxes">
                            <div class="card-group">
                                <div class="card">
                                    <div class="card-header" style="background-color: aquamarine">
                                        General Body
                                    </div>
                                    <div class="card-block chkboxes">
                                        <div class="input-group py-1 mt-2">
                                            <span class="input-group-addon">Habitus</span>
                                            <select class="form-control" id="ext-habitus">
                                                <option>Appears normally grown and developed</option>
                                                <option>Normally developed, but small for age</option>
                                                <option>Normally developed, but large for age</option>
                                            </select>
                                        </div>
                                        <div class="input-group py-1 mt-2">
                                            <span class="input-group-addon input">Skin / Soft tissue</span>
                                            <select class="form-control" id="ext-skin" multiple>
                                                <option selected>Unremarkable for age</option>
                                                <option>Generalized edema</option>
                                                <option>Icteric</option>
                                                <option>Macerated <50% of surface area</option>
                                                <option>Macerated >50% of surface area*</option>
                                            </select>
                                        </div>
                                        <div>
                                        <div class="input-group py-1 mt-2">
                                            <span class="input-group-addon">Extremities</span>
                                            <select class="form-control" id="ext-extremities">
