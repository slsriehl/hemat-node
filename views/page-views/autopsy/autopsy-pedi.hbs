<div class="container-fluid">
    <!-- Breadcrumbs -->
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="/">Home</a>
        </li>
        <li class="breadcrumb-item">
            Autopsy - Livebirth
        </li>
        <li class="breadcrumb-item active" id="bc-jump"><i class="fa fa-child mr-2"></i>Pediatric Autopsy - Perinatal, Livebrith and Infant</li>
    </ol>

    {{> message-center this}}

    <!-- Full width row + card -->
    <div class="row">
        <!-- top card-->
        <div class="col col-md-12">
            <div class="card">
                <div class="card-header">
                    <!-- inline header form-->
                    <form class="form-inline">

                        <div class="col col-md-4">
                            <button type="button" class="btn btn-primary btn-md p-1" data-toggle="modal"
                                    data-target="#pedi-autopsy-1">How to Use This App
                            </button>
                        </div>
                        <div class="col col-md-4">
                            <button type="button" class="btn btn-success btn-md p-1" data-toggle="modal"
                                    data-target="#pedi-autopsy-2">Printable Grossing Sheet
                            </button>
                        </div>
                        <!-- How to use Modal -->
                        {{> page-modals/autopsy-pedi/autopsy-pedi-how-to-use}}
                        <!-- Gross fixer Modal -->
                        {{> page-modals/autopsy-pedi/autopsy-pedi-gross-sheet}}


                    </form>
                    <!--Date-->

                </div>
                <div class="card-block">
                    <div class="card-header h5 my-2"><a href="#">Autopsy details</a></div>
                    <div class="row">
                        <div class="col col-md-4">
                            <!-- Birth/Death/Autopsy details-->
                            <div class="card">
                                <div class="card-header">
                                    Birth Details
                                </div>
                                <div class="card-block ">
                                    <div class="input-group date my-date-picker">
                                        <span class="input-group-addon">
                                            Birthdate
                                        </span>
                                        <input type="text" class="form-control form-control-sm" id="date-birth"/>
                                    </div>
                                    <div class="input-group pt-2 date my-time-picker">
                                        <span class="input-group-addon">
                                            Time of birth
                                        </span>
                                        <input type="text" class="form-control form-control-sm" id="date-btime"/>
                                    </div>
                                    <div class="input-group pt-2">
                                        <span class="input-group-addon">
                                            Sex
                                        </span>
                                        <select class="form-control" id="sex">
                                            <option>male</option>
                                            <option>female</option>
                                            <option>ambiguous</option>
                                        </select>
                                    </div>
                                    <div class="input-group pt-2">
                                        <span class="input-group-addon">
                                            Weight
                                        </span>
                                        <input type="text" class="form-control form-control-sm" id="weight"/>
                                        <span class="input-group-addon">
                                            <select id="weight-unit"><option>grams</option><option>kg</option></select>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class='col col-md-4'>
                            <div class="card">
                                <div class="card-header">
                                    Death details
                                </div>
                                <div class="card-block ">
                                    <div class="input-group date my-date-picker">
                                        <span class="input-group-addon">
                                            Date of death
                                        </span>
                                        <input type="text" class="form-control form-control-sm" id="date-death"/>
                                    </div>
                                    <div class="input-group date my-time-picker pt-2">
                                        <span class="input-group-addon">
                                            Time of death
                                        </span>
                                        <input type="text" class="form-control form-control-sm " id="date-dtime"/>
                                    </div>
                                    <div class="input-group pt-2">
                                        <span class="input-group-addon"
                                              data-placement="left" data-toggle="tooltip"
                                              title="Autocalculated, but you can enter your own age (enter 0 for stillbirth)" aria-hidden="true">
                                            Age
                                        </span>
                                        <input type="text" class="form-control form-control-sm" id="age"/>
                                    </div>
                                    <div class="input-group pt-2 ga" style="display: none;">
                                        <span class="input-group-addon"
                                              data-placement="left" data-toggle="tooltip"
                                              title="Enter gestational age for stillbirths" aria-hidden="true">
                                            Gestational Age
                                        </span>
                                        <input type="text" class="form-control form-control-sm" id="age-ga"/>
                                    </div>
                                    <div class="input-group py-2 date my-date-picker">
                                        <span class="input-group-addon">
                                            Date of autopsy
                                        </span>
                                        <input type="text" class="form-control form-control-sm" id="date-autopsy"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col col-md-4">
                            <div class="card">
                                <div class="card-header">
                                    Autopsy performance
                                </div>
                                <div class="card-block ">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            Autopsy type
                                        </span>
                                        <select class="form-control" id="autopsy-type">
                                            <option>No restrictions</option>
                                            <option>Limited</option>
                                        </select>
                                    </div>
                                    <div class="input-group pt-2">
                                        <span class="input-group-addon">
                                            Limitations?
                                        </span>
                                        <input class="form-control form-control-sm" id="autopsy-limits" placeholder="Leave blank if none">
                                    </div>
                                    <div class="input-group pt-2">
                                        <span class="input-group-addon">
                                            Pathologist
                                        </span>
                                        <input class="form-control form-control-sm" id="autopsy-md" placeholder="Name of pathologist">
                                    </div>
                                    <div class="input-group pt-2">
                                        <span class="input-group-addon">
                                            Physician notified
                                        </span>
                                        <input class="form-control form-control-sm" id="autopsy-notify" placeholder="Name of physician notified prior to prosection">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row top-buffer">
        <div class="col-md-8">
            <!-- left column-->


            <!-- left column accordion-->
            <div id="accordion" role="tablist" aria-multiselectable="true">
