                                            <div class="input-group py-1 mt-2">
                                                <span class="input-group-addon">HepB</span>
                                                <select class="form-control " id="maternal-hepb">
                                                    <option>Negative</option>
                                                    <option>Positive</option>
                                                    <option>Unknown</option>
                                                </select>
                                            </div>
                                            <div class="input-group py-1 mt-2">
                                                <span class="input-group-addon">HIV</span>
                                                <select class="form-control " id="maternal-hiv">
                                                    <option>Negative</option>
                                                    <option>Positive</option>
                                                    <option>Unknown</option>
                                                </select>
                                            </div>
                                            <div class="input-group py-1 mt-2">
                                                <span class="input-group-addon">Rubella</span>
                                                <select class="form-control " id="maternal-rubella">
                                                    <option>Immune</option>
                                                    <option>Non-immune</option>
                                                    <option>Unknown</option>
                                                </select>
                                            </div>
                                            <div class="input-group py-1 mt-2">
                                                <span class="input-group-addon">GBS</span>
                                                <select class="form-control " id="maternal-gbs">
                                                    <option>Not detected</option>
                                                    <option>Present</option>
                                                    <option>Unknown</option>
                                                </select>
                                            </div>
                                            <div class="text-center bg-info w-100 p-1 my-1">Labor information</div>
                                            <div class="w-100 py-1 mt-2">Prenatal ultrasound (if known)</div>
                                            <div class="input-group date my-date-picker mt-2">
                                                <span class="input-group-addon">
                                                    Date of sonograph
                                                </span>
                                                <input type="text" class="form-control form-control-sm" id="maternal-sono-date"/>
                                            </div>
                                            <div class="input-group py-1 mt-2">
                                                <span class="input-group-addon">
                                                    Findings
                                                </span>
                                                <select class="form-control " id="maternal-sono-findings">
                                                    <option>Size appropriate for gestational age</option>
                                                    <option>Unknown</option>
                                                    <option>Other</option>
                                                </select>
                                            </div>
                                            <div class="py-1 mt-1 sono-other" style="display: none;">
                                                <span class="w-100 py-1">
                                                    Describe other sonographic findings
                                                </span>
                                                <textarea class="form-control txtbox" id="maternal-sono-other" rows = "8"></textarea>
                                            </div>
                                            <div class="input-group py-1 mt-2">
                                                    <span class="input-group-addon">
                                                        Labor details
                                                    </span>
                                                <select class="form-control " id="maternal-labor">
                                                    <option>Progressed expectantly</option>
                                                    <option>Induced</option>
                                                </select>
                                                <select class="form-control " id="maternal-delivery">
                                                    <option>Vaginal delivery</option>
                                                    <option>Cesarean section</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card hosp-course">
                                    <div class="card-header">
                                        Describe the hospital course
                                    </div>

                                    <div class="card-block chkboxes">
                                        <div class="text-center bg-warning w-100 p-1 my-1">Hospital course</div>
                                        <textarea class="form-control w-100 txtbox" rows="50" id="hospital-course"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Weights and measurements-->
                <div class="card">
                    <div class="card-header" role="tab" id="headingTwo">
                        <h5 class="mb-0">
